@{
    ViewData["Title"] = "Home Page";
}


<form method="post">
    <div class="form-row">
        <div class="col-3">
            <input type="text" name="namecar" class="form-control" placeholder="Name Car">
        </div>
        <div class="col-3">
            <input type="text" name="color" class="form-control" placeholder="Color">
        </div>
        <div class="col-3">
            <input type="text" name="manuf" class="form-control" placeholder="Manufacturer">
        </div>
        <div class="col-3">
            <input type="number" name="year" min="1900" class="form-control" placeholder="Year">
        </div>
        <div class="col-3">
            <input type="number" name="engvol" class="form-control" placeholder="EngineVolume">
        </div>
        <div class="col-2">
            <input type="submit" class="form-control" placeholder="Save">
        </div>
    </div>
</form>
@model List<string>
@if (Model != null)
{
    // Model as string
    @foreach (var item in Model)
    {
        <p>
            @item.ToString();
        </p>
    }

}